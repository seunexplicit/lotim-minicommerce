  "description": "lotim vet mini ecommerce backend system",
  "main": "index.js",
     "scripts": {
          "test": "echo \"Error: no test specified\" && exit 1",
          "debug": "export DEBUG=* && npm run start",
          "start": "pm2-runtime start ecosystem.config.js --env production --only production",
          "start:dev": "pm2-runtime start ecosystem.config.js --env development --only development",
          "postinstall": "tsc",
          "deploy": "git add . && git commit -m Heroku && git push heroku main",
          "heroku-postbuild": "node post-install.js && npm install pm2"
